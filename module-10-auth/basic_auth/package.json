{
	"name": "wbs-node-ts-template",
	"version": "1.0.0",
	"description": "",
	"main": "dist/app.js",
	"type": "module",
	"imports": {
		"#utils": {
			"development": "./src/utils/index.ts",
			"default": "./dist/utils/index.js"
		},
		"#db": {
			"development": "./src/db/index.ts",
			"default": "./dist/db/index.js"
		},
		"#models": {
			"development": "./src/models/index.ts",
			"default": "./dist/models/index.js"
		},
		"#controllers": {
			"development": "./src/controllers/index.ts",
			"default": "./dist/controllers/index.js"
		},
		"#middlewares": {
			"development": "./src/middlewares/index.ts",
			"default": "./dist/middlewares/index.js"
		},
		"#config": {
			"development": "./src/config/index.ts",
			"default": "./dist/config/index.js"
		}
	},
	"scripts": {
		"dev": "node --env-file=.env.development.local --watch --conditions development src/app.ts",
		"prebuild": "rm -rf dist",
		"build": "tsc",
		"prestart": "npm run build",
		"start": "node --env-file=.env.production.local dist/app.js"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/bcrypt": "^6.0.0",
		"@types/cookie-parser": "^1.4.10",
		"@types/express": "^5.0.6",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/mongoose": "^5.11.96",
		"@types/node": "^24.0.14",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"bcrypt": "^6.0.0",
		"cookie-parser": "^1.4.7",
		"express": "^5.2.1",
		"jsonwebtoken": "^9.0.3",
		"mongoose": "^9.1.2",
		"zod": "^4.3.5"
	}
}
